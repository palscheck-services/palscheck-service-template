{
  "name": "palscheck-service-template",
  "version": "1.0.0",
  "main": "server.ts",
  "repository": "git@github.com:palscheck-services/palscheck-service-template.git",
  "license": "MIT",
  "dependencies": {
    "@types/morgan": "1.9.4",
    "bcryptjs": "2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "4.1.1",
    "dotenv": "^16.0.3",
    "envsafe": "^2.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "http-errors": "2.0.0",
    "husky": "8.0.3",
    "jsonwebtoken": "9.0.0",
    "lint-staged": "13.1.0",
    "mongoose": "^6.8.4",
    "morgan": "1.10.0",
    "nodemon": "^2.0.20",
    "redis": "4.6.7"
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.20.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "build": "yarn install && yarn build:clean && yarn build:compile",
    "build:clean": "yarn clean",
    "build:compile": "tsc",
    "clean": "rm -rf build",
    "dev": "nodemon --watch './**/*.ts' --exec ts-node src/server.ts",
    "lint": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
    "lint:check": "eslint --ignore-path .eslintignore \"src/**/*.ts\"",
    "prepare": "husky install",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "start": "node build/src/server.js",
    "test": "jest --watch --runInBand"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/http-errors": "2.0.1",
    "@types/jsonwebtoken": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "^29.3.1",
    "prettier": "2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
